%table.datatabler2.table.table-bordered.table-hover{style: "width:100%;cellspacing:0px;"}
  %thead
    %tr
      %th 
      - @deltahasharray["curveDeltas"].keys.each do |item|
        %th=item
        %th

  %tbody
    - counter = 0
    - @deltahasharray["periods"].each_with_index do |item, index|
      %tr
        %td=item
        - if counter < @curveDeltaCount
          - if index < (@objectcount - 1)

            %td= @deltaobjects[@deltakeys[counter]][0][index]
            %td= @deltaobjects[@deltakeys[counter]][1][index]

          - elsif index == (@objectcount - 1)
            %td= @deltaobjects[@deltakeys[counter]][0][index]
            %td= @deltaobjects[@deltakeys[counter]][1][index]
            -counter+=1
          - else
            %td No Info available
            %td


:javascript
  $('.datatabler2').DataTable({
      "searching": false,
      "lengthChange": false,
      "info": false,
      "order": []
  });



