%table.table.table-bordered.table-hover{style: "width:100%;cellspacing:0px;overflow:auto;max-width:100%;max-height:300px", id: "#{@bodyidentifier}_table"}
  %thead
    %tr
      %th 
      - @deltakeys.each do |item|
        %th=item
        %th

  %tbody
    - @deltahasharray["periods"].each_with_index do |item, index|
      %tr
        %td=item
        - (0..(@deltakeys.length-1)).each do |indexer|
          %td= @deltaobjects[@deltakeys[indexer]][0][index]
          %td= @deltaobjects[@deltakeys[indexer]][1][index]

:javascript
  $('##{@bodyidentifier}_table').DataTable({
      "responsive": true,
      "searching": false,
      "lengthChange": false,
              "scrollY":        "200px",
      "scrollCollapse": true,
      "paging": false,
      "info": false,
      "bSort": false

  });



